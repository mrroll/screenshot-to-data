{
  "name": "api",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "npm-run-all --sequential lint build:clean build:typescript build:alias",
    "build:alias": "tsc-alias --project tsconfig.build.json --verbose",
    "build:clean": "rimraf ./build",
    "build:typescript": "cross-env NODE_ENV=production TZ=UTC tsc --project tsconfig.build.json",
    "dev": "cross-env NODE_ENV=development TZ=UTC tsx watch --require dotenv-expand/config --inspect=127.0.0.1:0 ./src/index.ts",
    "lint": "npx eslint './src/**/**'",
    "start": "cross-env NODE_ENV=production TZ=UTC tsx --require dotenv-expand/config ./build/index.js",
    "test": "cross-env TZ=UTC vitest run --reporter verbose",
    "test:watch": "cross-env TZ=UTC vitest watch"
  },
  "dependencies": {
    "@fastify/helmet": "^11.1.1",
    "@fastify/swagger": "^8.14.0",
    "@fastify/swagger-ui": "^3.0.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "dotenv-expand": "^10.0.0",
    "fastify": "^4.26.2",
    "fastify-plugin": "^4.5.1",
    "fastify-type-provider-zod": "^1.1.9",
    "http-errors": "^2.0.0",
    "ky": "^1.2.3",
    "node-graceful": "^3.1.0",
    "npm-run-all": "^4.1.5",
    "zod": "^3.22.4",
    "zx": "^7.2.3"
  },
  "devDependencies": {
    "@pluslabs/style-guide": "^0.3.0",
    "@types/http-errors": "^2.0.4",
    "@types/node": "^20.12.6",
    "eslint": "^8.57.0",
    "prettier": "^3.2.5",
    "rimraf": "^3.0.2",
    "tsc-alias": "^1.8.8",
    "tsx": "^4.7.2",
    "type-fest": "^4.15.0",
    "typescript": "^5.4.4",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "1.4.0"
  },
  "volta": {
    "extends": "../../package.json"
  }
}
